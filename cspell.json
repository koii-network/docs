  "caseSensitive": false,
  "flagWords": [
      "hte"
  ],
  "patterns": [
    {
        "name": "comment-single-line",
        "pattern": "/#.*/g"
    },
    {
        "name": "comment-multi-line",
        "pattern": "/(?:\\/\\*[\\s\\S]*?\\*\\/)/g"
    },

    {
        "name": "comments",
        "pattern": ["comment-single-line", "comment-multi-line"]
    },
    {
      "name": "code-block",
      "pattern": "/```[\\s\\S]*?```/gmx"
    },
    {
      "name": "one-line-code-block",
      "pattern": "/`[^`]*`/g"
    },
    {
      "name": "keys",
      "pattern": " /[a-zA-Z0-9]{43,44}/g"
    },
    {
      "name": "capital",
      "pattern": "/\b[A-Z]+\b/g"
    }
],

"ignoreRegExpList": ["comments", "code-block", "one-line-code-block","keys"]

}
